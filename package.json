{
	"name": "isp",
	"displayName": "isp",
	"description": "stm32 isp download tool",
	"version": "0.0.7",
	"repository": "https://github.com/mbossX/isp-vscode.git",
	"publisher": "jiyun-tech",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Other"
	],
	"icon": "icon.png",
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "isp.execute",
				"title": "isp download"
			}
		],
		"configuration": {
			"title": "isp",
			"properties": {
				"isp.hex": {
					"type": "string",
					"scope": "machine",
					"markdownDescription": ".hex file path to download",
					"default": ""
				},
				"isp.com": {
					"type": "string",
					"scope": "machine",
					"markdownDescription": "serial port",
					"default": "COM3"
				},
				"isp.baud": {
					"type": "number",
					"scope": "machine",
					"markdownDescription": "serial baud rate",
					"default": 115200
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"login": "vsce login jiyun-tech cjfsaytnv2wjdzcbj4e33kn4vgjcixn73cochiauohexayp7b6mq",
		"publish": "vsce publish"
	},
	"dependencies": {
		"serialport": "9.2.8"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.63.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"electron-rebuild": "^3.2.5",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"ts-loader": "^9.2.5",
		"typescript": "^4.4.4",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	}
}
